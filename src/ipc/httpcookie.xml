        filename="httpcookie"
        list="public"
        list2="public" >
    <headers>
      <header type="sys" >gwenhywfar/types.h</header>
      <header type="sys" >gwenhywfar/gwentime.h</header>
    </headers>
    
    <elem name="name" type="char" ptr="1" />
    <elem name="value" type="char" ptr="1" />
    <elem name="expires" type="GWEN_TIME" ptr="1" />
    <elem name="path" type="char" ptr="1" />
    <elem name="domain" type="char" ptr="1" />
    <elem name="secure" type="int" />
  </type>
  
  
  
</types>
