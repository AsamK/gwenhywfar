
<GROUPS>
	<GROUPdef id="SegHead" delimiter=":" >
    <ELEM name="code" type="AN" maxsize="8"/>
    <ELEM name="seq" type="num" maxsize="15" topdown="1" />
    <ELEM name="version" type="num" maxsize="3"/>
    <ELEM name="ref" type="num" maxsize="3" minnum=0 />

   <VALUES>
      <VALUE path="seq">+SegmentNumber</VALUE>
   </VALUES>
  </GROUPdef>

  <GROUPdef id="CryptKey" delimiter=":" >
    <ELEM name="type" type="an" maxsize="16" />
    <ELEM name="owner" type="an" maxsize="16" />
    <ELEM name="number" type="num" maxsize="3" />
    <ELEM name="version" type="num" maxsize="3" />
  </GROUPdef>

  <GROUPdef id="PubRSACryptKey" delimiter=":" >
    <GROUP type="CryptKey" />
    <GROUP name="data" >
      <ELEM name="public" type="num" maxsize="1" />
      <ELEM name="n" type="bin" maxsize="256" />
      <ELEM name="e" type="bin" maxsize="256" />
    </GROUP>
  </GROUPdef>


  <GROUPdef id="PrivateRSACryptKey" delimiter=":" >
    <GROUP type="CryptKey" />
    <GROUP name="data" >
      <ELEM name="public" type="num" maxsize="1" />
      <ELEM name="n" type="bin" maxsize="256" />
      <ELEM name="e" type="bin" maxsize="256" />
      <ELEM name="p" type="bin" maxsize="256" />
      <ELEM name="q" type="bin" maxsize="256" />
      <ELEM name="dmp1" type="bin" maxsize="256" />
      <ELEM name="dmq1" type="bin" maxsize="256" />
      <ELEM name="iqmp" type="bin" maxsize="256" />
      <ELEM name="d" type="bin" maxsize="256" />
    </GROUP>
  </GROUPdef>


</GROUPS>


<SEGS>

 <SEGdef id="MsgHeadShort" version="1" delimiter="+" >
   <!-- "Nachrichtenkopf"beim Einlesen der Groesse -->
   <GROUP type="SegHead" name="head" />
   <ELEM name="size" type="num" maxsize="12" leftfill="1" />
   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="MsgHead" code="MSGHEAD" version="1"
         delimiter="+" terminator="'" >
   <GROUP type="SegHead" name="head" />
   <ELEM name="size" type="num" maxsize="12" leftfill="1" />
   <ELEM name="version" type="num" maxsize="12"/>
   <ELEM name="dialogId" type="num" maxsize="20" />
   <ELEM name="msgnum" type="num" maxsize="4" />

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>



 <SEGdef id="RQGreet" code="RQGREET" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <ELEM name="encrypt" type="num" maxsize="1" />
   <ELEM name="sign" type="num" maxsize="1" />
   <ELEM name="name" type="AN" maxsize="32"/>
   <ELEM name="version" type="ascii" maxsize="32"/>

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RPGreet" code="RPGREET" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <ELEM name="encrypt" type="num" maxsize="1" />
   <ELEM name="sign" type="num" maxsize="1" />
   <ELEM name="name" type="AN" maxsize="32"/>
   <ELEM name="version" type="ascii" maxsize="32"/>

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RQGetPK" code="RQGETPK" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RPGetPK" code="RPGETPK" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <GROUP type="PubRSACryptKey" name="key" />

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RQSetPK" code="RQSETPK" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <GROUP type="PubRSACryptKey" name="key" />

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RPSetPK" code="RPSETPK" version="1"
         delimiter="+" terminator="'" crypt="0" sign="0" >
   <GROUP type="SegHead" name="head" />
   <ELEM name="result" type="num" />

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>


 <SEGdef id="RQClose" code="RQCLOSE" version="1"
         delimiter="+" terminator="'"  encrypt="0" >
   <GROUP type="SegHead" name="head" />
    <ELEM name="reason" type="ascii" maxsize="64"/>

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </SEGdef>

</SEGS>

